# Stockfish binary path (defaults to `stockfish` on PATH)
STOCKFISH_PATH=stockfish
# Stockfish depth (higher = slower but stronger)
STOCKFISH_DEPTH=12
# Max moves to analyze per game
MAX_MOVES=120

# Gemini configuration
GEMINI_API_KEY=your_api_key_here
# Primary coaching model (text generation)
GEMINI_PRIMARY_MODEL=gemini-3-flash-preview
# Formatter model (structured JSON formatting)
GEMINI_FORMATTER_MODEL=gemini-2.5-flash
# Set to json (default) or text
COACH_RESPONSE_MODE=json
GEMINI_TEMPERATURE=0.2
